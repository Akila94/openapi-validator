{
  "name": "swagger-validator-ibm",
  "description": "Swagger Validator",
  "version": "1.0.4",
  "main": "src/index.js",
  "repository": "https://github.ibm.com/dustinpopp/swagger-validator-ibm.git",
  "license": "Apache-2.0",
  "scripts": {
    "prepush": "npm test",
    "build": "babel src -d dist/src --quiet && babel src/.defaultsForValidator.js -o dist/src/.defaultsForValidator.js --quiet && npm i -g",
    "test": "npm run just-test-in-node && npm run test-cli-tool",
    "just-test-in-node": "mocha --recursive --compilers js:babel-core/register test/plugins",
    "debug-test-in-node": "mocha --inspect --debug-brk --recursive --compilers js:babel-core/register test/plugins",
    "test-cli-tool": "mocha --compilers js:babel-core/register test/cli-validator/tests"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.1.0",
    "commander": "^2.11.0",
    "globby": "^7.1.1",
    "immutable": "^3.x.x",
    "js-yaml": "^3.5.5",
    "json-dup-key-validator": "^1.0.2",
    "jsonschema": "^1.1.0",
    "lodash": "^4.17.4",
    "matcher": "^1.0.0",
    "pad": "^2.0.3",
    "require-all": "^2.2.0",
    "semver": "^5.4.1",
    "swagger-parser": "^3.4.2",
    "yaml-js": "^0.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "expect": "^21.2.1",
    "husky": "^0.14.3",
    "intercept-stdout": "^0.1.2",
    "mocha": "^3.2.0",
    "strip-ansi": "^4.0.0"
  },
  "bin": {
    "lint-swagger": "./dist/src/cli-validator/index.js"
  }
}
