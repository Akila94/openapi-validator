{
  "name": "swagger-validator-ibm",
  "description": "Swagger Validator",
  "version": "1.2.1",
  "main": "dist/src/lib/index.js",
  "repository": "https://github.ibm.com/dustinpopp/swagger-validator-ibm.git",
  "license": "Apache-2.0",
  "scripts": {
    "prepush": "npm test",
    "build": "babel src -d dist/src --quiet && babel src/.defaultsForValidator.js -o dist/src/.defaultsForValidator.js --quiet",
    "prepare": "npm run build",
    "build-and-link": "npm run build && npm i -g",
    "test": "npm run lint && npm run build && npm run test-validators && npm run test-cli-tool",
    "test-validators": "mocha --recursive --require babel-core/register test/plugins",
    "test-cli-tool": "mocha --require babel-core/register test/cli-validator/tests",
    "lint": "eslint --cache --quiet --ext '.js' src test"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.3.2",
    "commander": "^2.15.1",
    "find-up": "^2.1.0",
    "globby": "^7.1.1",
    "js-yaml": "^3.11.0",
    "json-dup-key-validator": "^1.0.2",
    "json-schema-ref-parser": "^5.0.3",
    "jsonschema": "^1.2.4",
    "lodash": "^4.17.5",
    "matcher": "^1.1.0",
    "pad": "^2.0.3",
    "require-all": "^2.2.0",
    "semver": "^5.5.0",
    "yaml-js": "^0.2.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "expect": "^22.4.3",
    "husky": "^0.14.3",
    "intercept-stdout": "^0.1.2",
    "mocha": "^5.0.5",
    "prettier": "^1.11.1",
    "strip-ansi": "^4.0.0"
  },
  "bin": {
    "lint-swagger": "./dist/src/cli-validator/index.js"
  }
}
